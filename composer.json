{
  "name": "jacobbuck/silverstripe-flags",
  "description": "Feature flag toggling for SilverStripe.",
  "license": "MIT",
  "type": "silverstripe-vendormodule",
  "homepage": "https://github.com/jacobbuck/silverstripe-flags",
  "keywords": ["feature", "features", "flag", "flags", "silverstripe", "switch", "toggle"],
  "authors": [
    {"name": "Jacob Buck", "email": "buck.jacob@gmail.com"}
  ],
  "support": {
    "issues": "https://github.com/jacobbuck/silverstripe-flags/issues"
  },
  "require": {
    "silverstripe/recipe-cms": "^4"
  },
  "autoload": {
    "psr-4:": {
      "JacobBuck\\Flags\\": "src/"
    }
  },
  "extra": {
    "installer-name": "silverstripe-flags",
    "expose": [
      "images"
    ],
    "screenshots": [
      "docs/images/screenshot1.png",
      "docs/images/screenshot2.png",
      "docs/images/screenshot3.png"
    ]
  }
}
